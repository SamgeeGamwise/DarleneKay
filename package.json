{
  "name": "eleventy-nunjucks-site",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:watch:sass\" \"npx @11ty/eleventy --serve\"",
    "build": "npm run build:sass && npx @11ty/eleventy",
    "watch:sass": "sass src/styles:_site/assets/css --style=expanded --watch",
    "build:sass": "sass src/styles:_site/assets/css --style=compressed"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@tailwindcss/cli": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "sass": "^1.92.1",
    "tailwindcss": "^4.1.13"
  },
  "dependencies": {
    "@tailwindplus/elements": "^1.0.13"
  }
}
